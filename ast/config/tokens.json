{
  "keywords": ["let", "const", "function", "class", "for"],
  "markers":{
    "calcuters":["+", "-", "*", "/", "//", "%", "**"],
    "comparisers": ["==", "!=", "<>", "<", ">", "<=", ">=", "===", "!==", "<>"],
    "assigners": ["=", "+=", "-=", "*=", "/=", "%=", "**=", "//="],
    "bitculters": ["&", "|", "^", "~", "<<", ">>"],
    "logicters": ["&&", "||", "!"],
    "separators": [";", ","],
    "comenter": ["/*", "*/"],
    "other": ["++", "--"],
    "all-punctuations": ["~`!@#$%^&*()_-+={}[]|\\:;\"'<>,.?/"]
  },
  "comments": [{
    "start": "///",
    "end": "///",
    "type": "line"
  },{
    "start": "#",
    "end": "\n",
    "type": "line"
  },{
    "start": "//",
    "end": "\n",
    "type": "line"
  },{
    "start": "/*",
    "end": "*/",
    "type": "block"
  }],
  "strings": [{
    "start": "'",
    "end": "'",
    "type": "single_quotation"
  },{
    "start": "\"",
    "end": "\"",
    "type": "double_quotation"
  }]
}